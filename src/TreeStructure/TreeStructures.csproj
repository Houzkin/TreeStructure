<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>TreeStructures</Title>
    <Authors>houzkin</Authors>
    <Copyright>houzkin</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Description>各ノードが親と子の参照を保持するツリー構造を提供します。
Provides a tree structure where each node holds parent and children references.</Description>
    <RepositoryUrl>https://github.com/Houzkin/TreeStructure</RepositoryUrl>
    <PackageTags>tree;hierarchy;structure;</PackageTags>
    <PackageIcon>treestrucuture.jpg</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Bindables\" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\icon\treestrucuture.jpg">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
